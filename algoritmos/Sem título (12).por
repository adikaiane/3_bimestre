programa {
  funcao inicio() {
    real num1,num2
    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2)
    se(num1<num2)
    escreva("O número ", num1, " vem antes do número ", num2)
    se(num1>num2)
    escreva("O número ", num2, " vem antes do número ", num1)
  }
}
